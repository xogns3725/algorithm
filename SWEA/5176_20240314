T = int(input())

def pre(n):
    global cnt
    if n>N:
        return
    # 왼쪽 트리
    pre(n*2)
    tree[n] = cnt
    cnt += 1
    # 오른쪽 트리
    pre(n*2+1)

for test_case in range(1, T + 1):
    N = int(input())
    cnt = 1
    tree = [0] * (N+1)
    pre(1)

    print(f'#{test_case} {tree[1]} {tree[N//2]}')